{
  "name": "json-schema-to-zod-piped",
  "type": "module",
  "version": "0.9.7",
  "description": "A wrapper around json-schema-to-zod for using stdin / stdout",
  "author": "Fabian <spiele2014fs@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "zod",
    "json-schema"
  ],
  "scripts": {
    "show-help": "tsc && node src/main.js --help",
    "build": "tsc",
    "intro": "node help.js",
    "format": "prettier --write src"
  },
  "files": [
    "src/**/*.js",
    "README.md"
  ],
  "contributors": [
    "Stefan Terdell (https://github.com/StefanTerdell)",
    "Chen (https://github.com/werifu)",
    "Nuno Carduso (https://github.com/ncardoso-barracuda)",
    "Lars Strojny (https://github.com/lstrojny)",
    "Navtoj Chahal (https://github.com/navtoj)",
    "Ben McCann (https://github.com/benmccann)",
    "Dmitry Zakharov (https://github.com/DZakh)",
    "Michel Turpin (https://github.com/grimly)"
  ],
  "bin": "./src/main.js",
  "dependencies": {
    "ajv": "^8.12.0",
    "buffer-split": "^1.0.0",
    "json-refs": "^3.0.15",
    "json-schema-to-zod": "https://github.com/HeroClay/json-schema-to-zod",
    "prettier": "^2.4.1",
    "zod": "^3.21.4",
    "zod-opts": "^0.1.6"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.9",
    "@types/buffer-split": "^1.0.0",
    "@types/prettier": "^2.4.1",
    "typescript": "^5.1.6"
  }
}
